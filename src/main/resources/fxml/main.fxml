<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Controller">
   <children>
      <Button defaultButton="true" layoutX="408.0" layoutY="83.0" mnemonicParsing="false" onAction="#buttonClick" text="Нажми сюда!" />
      <TextField fx:id="textField" layoutX="22.0" layoutY="83.0" onKeyReleased="#checkText" prefHeight="26.0" prefWidth="372.0" />
      <Label fx:id="outputLabel" layoutX="22.0" layoutY="125.0" prefHeight="16.0" prefWidth="499.0" />
      <Button fx:id="button" disable="true" layoutX="22.0" layoutY="157.0" mnemonicParsing="false" onAction="#hello" prefHeight="26.0" prefWidth="499.0" text="Button" />
      <TextArea fx:id="textArea" layoutX="22.0" layoutY="200.0" prefHeight="182.0" prefWidth="499.0">
         <font>
            <Font name="Courier New" size="13.0" />
         </font></TextArea>
   </children>
</AnchorPane>
